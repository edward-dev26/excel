{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "operator-linebreak": "off",
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "max-len": ["error", { "code": 120 }],
    "eol-last": ["error", "never"],
    "no-debugger": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "plugins": ["jest"]
}