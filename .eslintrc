{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "indent": ["error", 4],
    "eol-last": ["error", "never"]
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}